{
  "version": "1.0.1",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Proxy Switch Pro",
  "short_name": "proxy-switch-pro",
  "permissions": ["proxy", "storage", "notifications"],
  "homepage_url": "https://electricalcodes.blogspot.com",
  "optional_permissions": ["*://*/*", "webRequest", "webRequestBlocking"],
  "description": "Switching occures between multiple proxies via toolbar popup menu.",
  "background": {
    "persistent": false,
    "scripts": [
      "lib/proxy.js",
      "lib/config.js", 
      "lib/firefox.js",
      "lib/runtime.js",
      "lib/webrequest.js",
      "lib/common.js"
    ]
  },
  "browser_action": {
    "default_title": "Proxy Switch Pro",
    "default_popup": "data/popup/popup.html",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
